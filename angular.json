{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nobleledger": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "inlineTemplate": false,
          "inlineStyle": true,
          "skipTests": true,
          "flat": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist"
            },
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "apexcharts",
              "jszip",
              "highlight.js",
              "crypto-js/enc-utf8",
              "crypto-js/hmac-sha256",
              "crypto-js/enc-base64",
              "flat",
              "excels",
              "dompurify",
              "html2canvas",
              "xlsx",
              "pdfmake",
              "file-saver",
              "moment",
              "moment-timezone",
              "moment-duration-format",
              "moment-timezone",
              "moment-duration-format",
              "jspdf-html2canvas",
              "rgbcolor",
              "core-js",
              "jspdf-autotable",
              "raf"              
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              {
                "glob": "_redirects",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@taiga-ui/icons/src",
                "output": "assets/taiga-ui/icons"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/@fuse/styles"
              ]
            },
            "styles": [
              "src/@fuse/styles/tailwind.scss",
              "src/@fuse/styles/themes.scss",
              "src/styles/vendors.scss",
              "src/@fuse/styles/main.scss",
              "src/styles/styles.scss",
              "src/styles/tailwind.scss",
              "@angular/material/prebuilt-themes/deeppurple-amber.css",
              "node_modules/@taiga-ui/core/styles/taiga-ui-theme.less",
              "node_modules/@taiga-ui/core/styles/taiga-ui-fonts.less",
              "node_modules/handsontable/dist/handsontable.full.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "15mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "75kb",
                  "maximumError": "90kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "nobleledger:build:production",
              "port": 43221
            },
            "development": {
              "buildTarget": "nobleledger:build:development",
              "port": 43221
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "nobleledger:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon-16x16.png",
              "src/favicon-32x32.png",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/styles/styles.scss"
            ],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "version": 2,
            "browserTarget": "nobleledger:build:production"
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "nobleledger:ionic-cordova-build",
            "devServerTarget": "nobleledger:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "nobleledger:ionic-cordova-build:production",
              "devServerTarget": "nobleledger:serve:production"
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "nobleledger:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "nobleledger:build:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "cache": false,
    "schematicCollections": [
      "@ionic/angular-toolkit"
    ]
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}

